{
  "name": "quantme-modeler-builder",
  "version": "0.0.0",
  "description": "QuantME Modeling and Transformation Framework based on the Camunda Modeler",
  "scripts": {
    "app:dev": "electron ./app/dev.js --force-update-checks",
    "app:test": "nyc --reporter lcov --exclude \"app/test/**\" --exclude \"app/**/__tests__\" mocha --require ./app/test/expect \"app/**/*-spec.js\"",
    "client:build": "(cd client && npm run build)",
    "client:dev": "(cd client && npm run dev)",
    "client:test": "(cd client && npm run test)",
    "plugin": "run-s plugin:quantme plugin:linter plugin:linter-extension",
    "plugin:quantme": "(cd resources/plugins/QuantME-CamundaPlugin && npm install && npm run build)",
    "plugin:linter": "(cd resources/plugins/Linter-plugin && npm install && npm run all)",
    "plugin:linter-extension": "(cd resources/plugins/QuantME-Linter-Extension-Plugin && npm install && npm run all)",
    "all": "run-s clean lint test \"build -- {@}\" --",
    "clean": "del-cli 'dist/*' app/public",
    "dev": "run-p \"app:dev -- {@}\" client:dev --",
    "build:distro": "node tasks/distro.js",
    "build:test-distro": "node tasks/test-distro.js",
    "build": "run-s plugin client:build \"build:distro -- {@}\" \"build:test-distro -- {@}\" --",
    "lint": "eslint .",
    "postinstall": "lerna bootstrap",
    "test": "run-s *:test",
    "start": "run-s \"dev -- {@}\" --"
  },
  "author": "UST-QuAntiL <https://github.com/UST-QuAntiL>",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/UST-QuAntiL/QuantME-TransformationFramework",
  "repository": {
    "type": "git",
    "url": "https://github.com/UST-QuAntiL/QuantME-TransformationFramework"
  },
  "devDependencies": {
    "archiver": "^3.0.3",
    "builder-util": "^22.3.3",
    "chai": "^4.2.0",
    "cpx": "^1.5.0",
    "del": "^3.0.0",
    "del-cli": "^1.1.0",
    "diff2html": "^2.11.2",
    "electron": "^7.1.12",
    "electron-builder": "^22.3.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^0.2.1",
    "electron-publisher-s3": "^20.17.2",
    "electron-reloader": "^0.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-bpmn-io": "^0.11.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-license-header": "^0.2.0",
    "execa": "^1.0.0",
    "lerna": "^3.16.1",
    "license-checker": "^25.0.1",
    "mocha": "^5.2.0",
    "mri": "^1.1.4",
    "nodemailer": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "proxyquire": "^2.1.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "tar-stream": "^2.1.0",
    "which": "^1.2.0",
    "yauzl": "^2.10.0",
    "jszip": "^3.6.0"
  }
}
